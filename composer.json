{
    "name": "tito10047/ux-twig-component-asset",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Acme bundle description",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/asset-mapper": "^6.4|^7.0",
        "symfony/ux-twig-component": "^2.13",
        "symfony/asset": "^6.4|^7.0",
        "symfony/web-link": "^6.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "symfony/twig-bundle": "^6.4|^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tito10047\\UxTwigComponentAsset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tito10047\\UxTwigComponentAsset\\Tests\\": "tests/"
        }
    }
}
